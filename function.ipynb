   "source": [
    "def leapyear(year):\n",
    "    if year%4!=0:\n",
    "        print(False)\n",
    "    else:\n",
    "        if year%100==0 and year%400!=0:\n",
    "            print(False)\n",
    "        else:\n",
    "            print(True)\n",
    "        \n",
    "n=int(input(\"input year\"))\n",
    "leapyear(n)"
   ]
