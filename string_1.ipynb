    "import sys, ast\n",
    "name = input(\"please input string with _ \")\n",
    "begin=0\n",
    "index=name.find(\"_\")\n",
    "while index > -1 :\n",
    "    print(name[begin:index])\n",
    "    begin=index+1\n",
    "    index=name.find(\"_\",begin,len(name))\n",
    "    if index==-1:\n",
    "        print(name[begin:len(name)])\n",
    "print(\"end\")"
   ]
